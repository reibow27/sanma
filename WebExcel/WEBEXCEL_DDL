-- CREATE DATABASE  `webexcel`.`webexcel` /*!40100 DEFAULT CHARACTER SET utf8 */CREATE DATABASE webexcel;CREATE USER user IDENTIFIED BY [PASSWORD] 'password';_CREATE TABLE  `webexcel`.`content_tbl` (  `wtable_id` int(11) NOT NULL,  `item_id` int(11) NOT NULL,  `id` int(11) NOT NULL,  `content` varchar(5000) DEFAULT NULL,  `input_type` varchar(100) NOT NULL,  `master_big_id` int(11) DEFAULT NULL,  `master_small_id` int(11) DEFAULT NULL,  `create_date` date DEFAULT NULL,  `update_date` date DEFAULT NULL,  `cont_date` date DEFAULT NULL,  PRIMARY KEY (`wtable_id`,`item_id`,`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8;CREATE TABLE  `webexcel`.`item_mst` (  `big_id` int(11) NOT NULL,  `small_id` int(11) NOT NULL AUTO_INCREMENT,  `name` varchar(100) NOT NULL,  `create_date` date DEFAULT NULL,  `update_date` date DEFAULT NULL,  `color` varchar(20) DEFAULT NULL,  PRIMARY KEY (`small_id`)) ENGINE=InnoDB AUTO_INCREMENT=27 DEFAULT CHARSET=utf8;CREATE TABLE  `webexcel`.`item_tbl` (  `wtable_id` int(11) NOT NULL,  `id` int(11) NOT NULL AUTO_INCREMENT,  `name` varchar(100) NOT NULL,  `item_order` int(11) NOT NULL,  `input_type` varchar(100) NOT NULL,  `master_big_id` int(11) DEFAULT NULL,  `width` int(5) NOT NULL,  `create_date` date DEFAULT NULL,  `update_date` date DEFAULT NULL,  PRIMARY KEY (`id`)) ENGINE=InnoDB AUTO_INCREMENT=84 DEFAULT CHARSET=utf8;CREATE TABLE  `webexcel`.`mst_seq_tbl` (  `id` int(11) NOT NULL AUTO_INCREMENT,  `mst_seq` int(11) NOT NULL,  `create_date` date DEFAULT NULL,  `update_date` date DEFAULT NULL,  PRIMARY KEY (`id`)) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;CREATE TABLE  `webexcel`.`web_table_tbl` (  `id` int(11) NOT NULL AUTO_INCREMENT,  `name` varchar(100) NOT NULL,  `remarks` varchar(1000) DEFAULT NULL,  `cont_seq` int(11) NOT NULL,  `create_date` date DEFAULT NULL,  `update_date` date DEFAULT NULL,  PRIMARY KEY (`id`)) ENGINE=InnoDB AUTO_INCREMENT=52 DEFAULT CHARSET=utf8;